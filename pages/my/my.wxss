@import "../../common/style/style.wxss";
page{background-color: #f1f2f5;}
.my-head{height: 260rpx;overflow: hidden;}
.my-item{height: 35px;line-height: 35px;border-bottom: 1px solid #eee;overflow: hidden;}
.my-item .my-ico{width: 20px;height: 19px;margin: 8px 15px 0 0;}
.left_ico{margin-top: 2px;}
.my-item:nth-child(4), .my-list {margin-top: 10px;}
.my_bg{width: 100%;}
.my-info{position: absolute;top: 0;width: 100%;height: 100%;padding: 53rpx 30rpx 0;}
.my-info-right {padding: 10px 0 0 91px;color: #fff;}
.my_ava{width: 72px;height: 72px;border-radius: 50%;-webkit-border-radius: 50%;border: 2px solid #dedede;}
.my_name, .my-item .right-con text {font-size: 15px;}
.my_name{line-height: 2;}
.maskDialog{overflow:hidden;}
.dialoghead{height:50px;line-height:50px;background-color:#f7f7f9;font-size:18px;text-align:center;}
.dialoghead>image{width:12px;height:12px;position:absolute;top:10px;right:10px;}
.step-list{padding-top:20px;}
.step-list>.step-item{width:38%;float:left;padding:0 6%;font-size:12px;margin-bottom:20px;}
.step-item>image{width:50px;height:50px;display:block;margin:0 auto 10px;}
.m-tips{font-size:12px;color:#ff432a;padding:0 6% 10%;}



